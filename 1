Array.prototype.myFilter = function(callback){
  const newArr = [];
  const arr = this;

  for(let i = 0; i < arr.length; i++){
    const element = arr[i];

    if(callback(element)){
      newArr.push(element);
    }
  }
  return newArr;
}

const arr = [1,2,3,5,6,7,8,9];

const newArr = arr.myFilter(function(item) { 
  return item % 3 == 0;
});

console.log(newArr);
 
